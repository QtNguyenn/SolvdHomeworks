<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <File name="File" fileName="Homework5/resources/appLog.log">
      <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %msg%n" />
    </File>
  </Appenders>
  <Loggers>
    <Root level="error">
      <AppenderRef ref="File" />
    </Root>
  </Loggers>
</Configuration>